{
  "name": "blockgamings_api",
  "version": "0.0.1",
  "description": "Blockgaming.com API",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "dev": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "louis.nguyen <louis.nguyen@jetstudio.io>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@honeybadger-io/js": "^3.2.0",
    "@loopback/authentication": "^7.2.0",
    "@loopback/authentication-jwt": "^0.8.1",
    "@loopback/authorization": "^0.8.1",
    "@loopback/boot": "^3.3.0",
    "@loopback/core": "^2.15.0",
    "@loopback/cron": "^0.5.1",
    "@loopback/repository": "^3.5.0",
    "@loopback/rest": "^9.2.0",
    "@loopback/rest-explorer": "^3.2.0",
    "@loopback/service-proxy": "^3.1.0",
    "@slider-captcha/core": "^1.0.1",
    "axios": "^0.21.1",
    "circular-json": "^0.5.9",
    "crypto-js": "^4.0.0",
    "dataloader": "^2.0.0",
    "date-fns": "^2.21.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "loopback-connector-mongodb": "^6.0.0",
    "lru_map": "^0.4.1",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.5.0",
    "salted-sha1": "^1.0.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/testlab": "^3.3.0",
    "@types/circular-json": "^0.4.0",
    "@types/crypto-js": "^4.0.1",
    "@types/node": "^10.17.55",
    "@types/node-rsa": "^1.1.0",
    "@types/nodemailer": "^6.4.1",
    "@types/uuid": "^8.3.0",
    "@types/yaml": "^1.9.7",
    "eslint": "^7.22.0",
    "source-map-support": "^0.5.19",
    "tsc-watch": "^4.2.9",
    "typescript": "~4.2.3"
  }
}
